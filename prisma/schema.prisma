generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model menu {
  id           String   @id @default(cuid())
  userId       String   
  name         String
  description  String? 
  price        Float
  image        String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
